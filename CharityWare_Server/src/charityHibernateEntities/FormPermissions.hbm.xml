<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 
 <hibernate-mapping>
  <class name="charityHibernateEntities.FormPermissions" table="form_permissions">
      <meta attribute="class-description">
         This class contains the filled form detail. 
      </meta>
      <composite-id name="pk" class="charityHibernateEntities.FormPermissionsPK">
    		<key-many-to-one name="form" column="form_id" class="charityHibernateEntities.Form" />
    		<key-many-to-one name="user_type" column="user_type_id" class="charityHibernateEntities.UserType"/>
	</composite-id>
      <property name="permission" column="Permission" type="string"/>
      <property name="isActive" column="isActive" type="boolean"/>
      <property name="timestamp" column="TimeStamp" type="java.sql.Timestamp"/>
   </class>
 </hibernate-mapping>
 